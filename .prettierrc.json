{
  "plugins": [
    "@prettier/plugin-pug"
  ],
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "importOrderSeparation": true,
  "pugSingleQuote": false,
  "importOrder": [
    "^vite$",
    "<THIRD_PARTY_MODULES>",
    "@plugins",
    "@pages",
    "@components",
    "@utils",
    "@styles",
    "@assets",
    "^./|^../"
  ]
}